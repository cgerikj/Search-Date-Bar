{
  "name": "searchHelper",
  "version": "0.1.0",
  "description": "Unfuck google search",
  "options_page": "options.html",
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["http://*/*", "https://*/*"],
    "include_globs": ["http://www.google.*/*", "https://www.google.*/*"]
  }],
  "manifest_version": 2
}
